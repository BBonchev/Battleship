<% provide(:title, "Edit user") %>


    <div class="row">
      <div class="col-md-6 col-md-offset-3">
        <div class="panel panel-login">
          <div class="panel-heading">
        
        <br>       
    <%= form_for(@user) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <div  align="center"> <div class="userphoto"><h1></h1></div><br>
      <div class="textform"> <%= f.label :Nickname %> </div>
      <%= f.text_field :name, class: 'form-control' %><br>

      <div class="textform"><%= f.label :email %> </div>
      <%= f.email_field :email, class: 'form-control' %><br>

      <div class="textform"><%= f.label :password %> </div>
      <%= f.password_field :password, class: 'form-control',placeholder: "White for new password.."  %><br>

      <div class="textform"><%= f.label :password_confirmation, "Confirmation" %> </div>
      <%= f.password_field :password_confirmation, class: 'form-control', placeholder: "White for password confirmation.."  %><br><br>
        
      <%= f.submit "save changes", class: "form-control btn btn-register" %>
    <% end %>
  </div>
</div>